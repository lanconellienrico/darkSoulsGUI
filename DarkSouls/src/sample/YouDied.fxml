<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="125.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.YouDiedController">
   <children>
      <ImageView fitHeight="125.0" fitWidth="410.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resource/YOUDIED.jpg" />
         </image>
      </ImageView>
      <Button fx:id="registratiInClassifica" layoutX="14.0" layoutY="84.0" mnemonicParsing="false" text="REGISTRATI IN CLASSIFICA!" textFill="#9f0404" onAction="#stampaClassifica">
         <font>
            <Font name="Baskerville Old Face" size="13.0" />
         </font>
      </Button>
      <Button fx:id="backToMenuButton" layoutX="268.0" layoutY="84.0" mnemonicParsing="false" text="TORNA AL MENU" textFill="#9f0404" onAction="#backToMenu">
         <font>
            <Font name="Baskerville Old Face" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
